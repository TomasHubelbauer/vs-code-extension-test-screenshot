{
	"name": "vs-code-extension-test-screenshot",
	"displayName": "vs-code-extension-test-screenshot",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && cross-env CODE_DISABLE_EXTENSIONS=true node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/fs-extra": "^7.0.0",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/node-fetch": "^2.3.4",
		"@types/ps-list": "^6.2.1",
		"@types/ws": "^6.0.1",
		"cross-env": "^5.2.0",
		"fs-extra": "^8.0.1",
		"node-fetch": "^2.6.0",
		"ps-list": "^6.3.0",
		"ps-node": "^0.1.6",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"ws": "^7.0.0"
	}
}
